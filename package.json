    "mongoose": "^8.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "sinon": "^15.1.2",
    "supertest": "^6.3.3"
  }
